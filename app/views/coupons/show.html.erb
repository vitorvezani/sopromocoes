<div class="page-header">
  <%= link_to coupons_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_coupon_path(@coupon), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show coupon</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @coupon.name %></dd>

  <dt>Code:</dt>
  <dd><%= @coupon.code %></dd>

  <dt>Url:</dt>
  <dd><%= @coupon.url %></dd>

  <dt>Begin at:</dt>
  <dd><%= @coupon.begin_at %></dd>

  <dt>End at:</dt>
  <dd><%= @coupon.end_at %></dd>

  <dt>Enabled:</dt>
  <dd><%= @coupon.enabled %></dd>

  <dt>Rules url:</dt>
  <dd><%= @coupon.rules_url %></dd>

  <dt>Affiliate:</dt>
  <dd><%= @coupon.affiliate %></dd>

</dl>
